{
  "manifest_version": 2,
  "version": "1.3.20",
  "name": "YouTube Auto HD + FPS",
  "short_name": "YT Auto HD",
  "description": "__MSG_cj_i18n_02146__",
  "author": "avi12",
  "developer": {
    "name": "avi12",
    "url": "https://avi12.com/youtube-auto-hd"
  },
  "default_locale": "en",
  "content_security_policy": "default-src 'none'; child-src 'none'; manifest-src 'none'; media-src 'none'; prefetch-src 'none'; worker-src 'none'; connect-src 'none'; frame-src 'none'; font-src 'none'; object-src 'none'; img-src data:; script-src 'self'; script-src-elem 'report-sample' 'self'; script-src-attr 'none'; style-src 'self'; style-src-attr 'self'; style-src-elem 'self' 'unsafe-inline'; base-uri 'none'; form-action 'none'; frame-ancestors 'none'; block-all-mixed-content; upgrade-insecure-requests; sand-box allow-same-origin allow-scripts allow-popups; report-uri https://api.jeurissen.co/reports/csp/webext/ythd/v1_3_38/",
  "applications": {
    "gecko": {
      "id": "avi6106@gmail.com"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "avi6106@gmail.com"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "24": "icons/icon-24.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "https://www.youtube.com/*",
    "https://www.youtube-nocookie.com/*",
    "https://youtube.googleapis.com/*"
  ],
  "background": {
    "scripts": ["build/background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "popup.html",
    "browser_style": false
  },
  "options_page": "popup.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "19": "icons/icon-19.png",
      "24": "icons/icon-24.png",
      "32": "icons/icon-32.png",
      "38": "icons/icon-38.png",
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://www.youtube-nocookie.com/*",
        "https://youtube.googleapis.com/*"
      ],
      "js": ["build/yt-auto-hd-content-script-initialize.js"],
      "all_frames": true
    }
  ],
  "offline_enabled": true
}
