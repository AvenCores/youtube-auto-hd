{
  "name": "youtube-auto-hd-fps",
  "version": "1.6.6",
  "homepage": "https://avi12.com",
  "repository": "https://github.com/avi12/youtube-auto-hd",
  "author": "avi12 <avi6106@gmail.com>",
  "bugs": {
    "email": "avi6106@gmail.com"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "pack": "web-ext build --overwrite-dest --artifacts-dir dist_packed --source-dir dist --filename youtube_auto_hd_fps-{version}.zip",
    "pack-stores": "webext-store-incompat-fixer -i dist_packed/youtube_auto_hd_fps-{version}.zip --edge-locale-inclusions de,de_CH,de_AT,es,es_419,fr,fr_CA,hr,he,hu,it,ja,id,nl,pl,pt_BR,ru,zh_Hans,zh_Hant --stores chrome,edge,firefox,opera",
    "pack-self": "zip-self -o dist_packed/youtube_auto_hd_fps-{version}-source.zip",
    "build-pack": "pnpm run build && pnpm run pack && pnpm run pack-stores && pnpm run pack-self",
    "build-for-firefox": "build-for-browser --i dist_packed/youtube_auto_hd_fps-{version}.zip --browser=firefox --source",
    "set-version": "set-json-version -f package.json dist/manifest.json -v",
    "validate": "svelte-check",
    "run-browser": "web-ext run --no-config-discovery --start-url https://www.youtube.com/watch?v=1E_kBSka_ec --source-dir dist --verbose --target",
    "run-chromium": "pnpm run-browser chromium",
    "run-edge-windows": "\"\"%ProgramFiles(x86)%\\Microsoft\\Edge\\Application\\msedge.exe\"\" https://www.youtube.com/watch?v=dQw4w9WgXcQ --load-extension=%cd%\\dist"
  },
  "devDependencies": {
    "@mdi/js": "^6.9.96",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "~13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@tsconfig/svelte": "^3.0.0",
    "@types/chrome": "^0.0.191",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "build-for-browser": "github:avi12/build-for-browser",
    "eslint": "^8.19.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "~2.75.7",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "~7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.53.0",
    "set-json-version": "^2.1.0",
    "stylelint": "^14.9.1",
    "stylelint-config-ccb": "^1.27.1",
    "svelte": "~3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-materialify": "^0.3.11",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "web-ext": "^7.1.0",
    "webext-store-incompat-fixer": "0.3.1",
    "zip-self": "^0.5.2"
  }
}
